<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>nycuro</groupId>
  <artifactId>nycuro</artifactId>
  <name>HubCore</name>
  <version>1.0</version>
  <description>HubCore</description>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.3</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <filters>
            <filter>
              <artifact>*:*</artifact>
              <excludes>
                <exclude>META-INF/*.SF</exclude>
                <exclude>META-INF/*.DSA</exclude>
                <exclude>META-INF/*.RSA</exclude>
              </excludes>
            </filter>
          </filters>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>nukkit-repo</id>
      <url>https://repo.nukkitx.com/snapshot</url>
    </repository>
    <repository>
      <id>DaPorkchop_</id>
      <url>https://maven.daporkchop.net/</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>cn.nukkit</groupId>
      <artifactId>nukkit</artifactId>
      <version>1.0-SNAPSHOT</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>org.projectlombok</groupId>
      <artifactId>lombok</artifactId>
      <version>1.18.4</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>gt.creeperface.nukkit.scoreboardapi</groupId>
      <artifactId>ScoreboardAPI</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>E:\Proiecte\Java\Cores/lib/ScoreboardAPI.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>ru.nukkit.multipass</groupId>
      <artifactId>Multipass</artifactId>
      <version>1.0-SNAPSHOT</version>
      <scope>system</scope>
      <systemPath>E:\Proiecte\Java\Cores/lib/LuckPerms-Nukkit-4.2.51.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>gt.creeperface.holograms</groupId>
      <artifactId>Holograms</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>E:\Proiecte\Java\Cores/lib/Holograms.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.creeperface.nukkit.placeholderapi.PlaceholderPlugin</groupId>
      <artifactId>PlaceholderAPI</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>E:\Proiecte\Java\Cores/lib/PlaceholderAPI.jar</systemPath>
    </dependency>
  </dependencies>
  <properties>
    <maven.compiler.testSource>1.8</maven.compiler.testSource>
    <maven.compiler.testTarget>1.8</maven.compiler.testTarget>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
  </properties>
</project>

